<div class="container-fluid">
  <table class="table">
    <thead>
      <tr>
        <th>Login</th>
        <th>Phone number</th>
        <th>Admin status</th>
      </tr>
    </thead>  

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.login %> </td>
          <td><%= user.phone_number %></td>
          <td><%= user.admin %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>